// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Asset {
  id            Int       @id @default(autoincrement())
  name          String
  lat           Decimal
  long          Decimal
  category      String
  riskRating    Decimal
  rishFactor    Json
}
